{
  "name": "hie-openai-demo",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "start": "node server.js",
    "dev": "vite",
    "netlify:dev": "netlify dev --timeout=60",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "setup-do": "node scripts/setup-digitalocean.js",
