    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-session": "^1.17.3",
    "helmet": "^7.1.0",
    "multer": "^1.4.5-lts.1",
    "nano": "^10.1.2",
    "node-fetch": "^2.7.0",
    "openai": "^4.56.1",
    "pdf-parse-new": "^1.4.1",
    "pg": "^8.11.3",
